<%- include ../partials/header.ejs %>

	<h1>Select 3 - 5 images</h1>
	
	<div class="test-page">
		<div class="polaroid">
			<% images.forEach(function(image) { %>
					<img data-genres="<%= image.genreIds %>" class="test-images" src="<%= image.url %>">
			<% }) %>
		</div>
			<form method="POST" action="/users/<%= user._id %>/histories" id="historyForm">
				<button id="submitButton">Submit Selections</button>
			</form>
</div>



